{
  "name": "web-portal",
  "version": "0.1.0",
  "private": true,
  "description": "web-portal",
  "keywords": [],
  "author": "laxman@technavious.com",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technavious/latest-web-portal.git"
  },
  "bugs": {
    "url": "https://github.com/technavious/latest-web-portal/issues"
  },
  "main": "index.js",
  "scripts": {
    "commit": "node_modules/.bin/git-cz -S",
    "commit:amend": "git-cz -S -a --amend",
    "docker-compose:run": "docker-compose up -d --build",
    "watch": "npm-watch",
    "start": "react-scripts start",
    "start:prod": "yarn index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npx serve -s build -l 3000",
    "start:pm2": "./node_modules/.bin/pm2-runtime start ./pm2.json --env $NODE_ENV",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.10.0",
    "yarn": ">=1.22.10"
  },
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "@material-ui/core": "^4.12.4",
    "@react-google-maps/api": "^2.12.2",
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/leaflet": "1.7.11",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.7.14",
    "@types/redux-batched-subscribe": "^0.1.4",
    "@types/redux-logger": "^3.0.9",
    "axios": "^0.27.2",
    "bootstrap": "5.2.2",
    "date-fns": "2.29.3",
    "express": "^4.18.1",
    "html2canvas": "^1.4.1",
    "jest": "^29.0.2",
    "jest-html-reporter": "^3.6.0",
    "jspdf": "^2.5.1",
    "jwt-decode": "^3.1.2",
    "leaflet": "1.8.0",
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "path": "^0.12.7",
    "pm2": "^5.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-bootstrap": "^2.5.0",
    "react-confirm-alert": "^3.0.6",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-easy-diagram": "^0.11.3",
    "react-ga4": "^2.0.0",
    "react-icons": "^4.4.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-js-pagination": "^3.0.3",
    "react-leaflet": "4.0.0",
    "react-leaflet-kml": "2.1.0",
    "react-pdf": "^5.7.2",
    "react-phone-number-input": "^3.2.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.4.0",
    "react-select-search": "^3.0.10",
    "react-to-print": "^2.14.7",
    "react-toastify": "^9.0.8",
    "reactstrap": "^9.1.3",
    "redux": "^4.2.0",
    "redux-batched-subscribe": "^0.1.6",
    "redux-logger": "^3.0.6",
    "screw-filereader": "^1.4.3",
    "styled-components": "^5.3.5",
    "title-case": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "4.3.8",
    "prettier": "^2.7.1",
    "standard-version": "9.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "port": "8080",
    "HUSKY_SKIP_HOOKS": "1 git rebase",
    "access": "restricted",
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn lint:fix",
      "prepare-commit-msg": "exec < /dev/tty && yarn commit --hook || true"
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "revert",
        "section": "Reverts",
        "hidden": false
      },
      {
        "type": "feat",
        "section": "Features",
        "hidden": false
      },
      {
        "type": "fix",
        "section": "Bug Fixes",
        "hidden": false
      },
      {
        "type": "improvement",
        "section": "Feature Improvements",
        "hidden": false
      },
      {
        "type": "style",
        "section": "Styling",
        "hidden": false
      },
      {
        "type": "perf",
        "section": "Performance Improvements",
        "hidden": false
      },
      {
        "type": "chore",
        "section": "Others",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Docs",
        "hidden": true
      },
      {
        "type": "refactor",
        "section": "Code Refactoring",
        "hidden": true
      },
      {
        "type": "test",
        "section": "Tests",
        "hidden": true
      },
      {
        "type": "build",
        "section": "Build System",
        "hidden": true
      },
      {
        "type": "ci",
        "section": "CI",
        "hidden": null
      }
    ]
  }
}
